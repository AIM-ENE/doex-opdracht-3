@startuml

hide circle

class Client <<Entity>>

class Jaar <<Entity>>

rectangle "Aggregate" {
    class Werkweek <<AR>>

    class Dag <<Entity>> {
        maakAfspraak()
    }

    class Slot <<Value Object>> {
        slotnummer
    }

    class Afspraak <<Value Object>>
}

Afspraak "1" -r-> "  1..2" Slot
Dag "1" --> "\t\t40" Slot
Dag "\t\t\t1" --> "0..40" Afspraak

Werkweek "1" -l- "5" Dag
Afspraak "1" --> "1" Client

Jaar "1" -u- "48" Werkweek



@enduml